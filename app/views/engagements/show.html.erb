<div class="card ">
  <div class="card-body">
    <h5 class="card-title text-center mb-4">Informacion de Engament</h5>   
		<div class="cardSpaceLeft">   

			<div class="mb-5">
				<%= link_to 'Engagements', building_engagements_path(@building) %>
			</div>
			<p>

			<%# for engagement in @building.engagements %>
				  <strong>Ente Residencial:</strong>
				  <%= @engagement.building.name %>
				</p>

				<p>
				  <strong>Usuario Creador:</strong>
				  <%= @engagement.creator.present?  ? User.find(@engagement.creator).email.titleize : "Impossible" %>
				</p>
				<p>
				  <strong>Usuario Asociado:</strong>
				  <%= @engagement.user.email.titleize %>
				</p>

				<p>
				  <strong>Role de Usuario Asociado:</strong>
				  <%= @engagement.role.titleize %>
				</p>
				<p>
				  <strong>Comenzó el :</strong>
				  <%= @engagement.started_at.strftime('%d %b %Y %H:%M:%S') %>
				</p>
						<p>
				  <strong>Terminó el :</strong>
				  <%#= @engagement.ended_at.strftime('%d %b %Y %H:%M:%S') %>
				</p>

				<div class="my-5">
					<%= link_to 'Edit', edit_building_engagement_path(@building, @engagement) %> 
				<div>
				<%#= link_to 'Engagements', building_engagements_path(@building) %>
			
		</div>
	</div>
</div>

<br>
